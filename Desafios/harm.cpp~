#include <bits/stdc++.h>
using namespace std;

int main()
{
  int n, i, j, T;
  double count, ln;
  scanf("%d", &T);

  for (j = 0; j < T; j++)
  {
    scanf("%d", &n);
    printf("%d - ", n);
    count = 0;
    for (i = 2; i < n/2 + 1; i++)
      count += 1.0/i;
    ln = (log(((double) n)/2 + 1));
    printf("count = %f || ln = %f\n", count, ln);
  }
}
