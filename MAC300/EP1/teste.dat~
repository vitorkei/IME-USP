i = 0
    j = 0
        b[0] = b[0]/A[0][0]
        0 = 0 / 36
        b[0] = 0

i = 1
    j = 0
        b[1] = b[1] - A[1][0] * b[0]
        b[1] = 12 - -0,833333 * 0
        b[1] = 12
    b[1] = b[1] / A[1][1]
         = 12 / 9
    b[1] = 1.3333333

i = 2
    j = 0
        b[2] = b[2] - A[2][0] * b[0] - A[2][1] * b[1]
             = -7 - 0,666666 * 0 - (-0,666666 * 1,33333)
             = -7 + 2/3 * 4/3
             = -7 + 8/9
             = -6,11111111
    b[2] = b[2]/A[2][2]
         = -6.1111/1
         = -6.11111111
